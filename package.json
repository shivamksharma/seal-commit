{
  "name": "seal-commit",
  "version": "1.5.0",
  "description": "A production-ready CLI tool to automatically detect and block API keys, secrets, tokens, or credentials from being committed to Git repositories",
  "type": "module",
  "main": "src/cli.js",
  "bin": {
    "seal-commit": "./src/cli.js"
  },
  "scripts": {
    "start": "node src/cli.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --passWithNoTests",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch --passWithNoTests",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage --passWithNoTests",
    "lint": "eslint 'src/**/*.js' 'tests/**/*.js' --ext .js --no-error-on-unmatched-pattern",
    "lint:fix": "eslint 'src/**/*.js' 'tests/**/*.js' --ext .js --no-error-on-unmatched-pattern --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\"",
    "prepare": "husky install",
    "postinstall": "node scripts/postinstall.js",
    "prepublishOnly": "npm run test && npm run lint",
    "prepack": "npm run format:check"
  },
  "keywords": [
    "git",
    "security",
    "secrets",
    "cli",
    "pre-commit",
    "api-keys",
    "credentials",
    "git-hooks",
    "secret-detection",
    "entropy",
    "aws",
    "github",
    "stripe",
    "jwt",
    "devops"
  ],
  "author": "seal-commit contributors",
  "license": "MIT",
  "homepage": "https://github.com/seal-commit/seal-commit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/seal-commit/seal-commit.git"
  },
  "bugs": {
    "url": "https://github.com/seal-commit/seal-commit/issues"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "js-yaml": "^4.1.0",
    "husky": "^8.0.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "@jest/globals": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "src/",
    "scripts/",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "CONFIGURATION.md"
  ]
}