{
  "patterns": {
    "enabled": [
      "aws-access-key",
      "aws-secret-key",
      "google-api-key",
      "stripe-key",
      "github-token",
      "jwt-token"
    ]
  },
  "entropy": {
    "threshold": 4.0,
    "minLength": 20,
    "maxLength": 100
  },
  "ignore": {
    "files": [
      "*.min.js",
      "*.map",
      "package-lock.json"
    ],
    "directories": [
      "node_modules",
      "dist",
      "build"
    ]
  }
}